SUB ESP,4
MOV EAX,0
MOV [EBP-4],EAX
SUB ESP,4
MOV EAX,1
MOV [EBP-8],EAX
SUB ESP,4
MOV EAX,0
MOV [EBP-12],EAX
SUB ESP,4
MOV EAX,10
MOV [EBP-16],EAX
M0:
MOV EAX,[EBP-16]
CMP EAX,0
JE M1
MOV EAX,[EBP-4]
MOV EBX,[EBP-8]
ADD EAX,EBX
MOV [EBP-12],EAX
MOV EAX,[EBP-8]
MOV [EBP-4],EAX
MOV EAX,[EBP-12]
MOV [EBP-8],EAX
MOV EAX,[EBP-16]
MOV EBX,1
SUB EAX,EBX
MOV [EBP-16],EAX
JMP M0
M1:
